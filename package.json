    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loading": "^2.0.3",
